#include<stdio.h>
#include <unistd.h>
#include <string.h>


int main(int argc, char *argv[])
{
    char buf[100];


 if (argc !=-1)
    {
    
    for (int i = 1; i < argc; i++) {

	    write(1, argv[i],strlen(argv[i]));
	     write(1, " ",strlen(" "));
	    
    }
    
   	    write(1, "\n",strlen("\n"));
    }
    return 0;
    }
    

